{
  "name": "lenientjs",
  "main": "src/index.js",
  "version": "0.0.0",
  "description": "Atom integration for Lenient JS syntax",
  "homepage": "https://xixixao.github.com/lenientjs",
  "keywords": [
    "coffee",
    "coffeescript",
    "indentation",
    "javascript",
    "js",
    "lazy",
    "lenient",
    "syntax",
    "whitespace"
  ],
  "activationHooks": ["language-coffee-script:grammar-used"],
  "repository": "https://github.com/xixixao/atom-lenientjs",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
